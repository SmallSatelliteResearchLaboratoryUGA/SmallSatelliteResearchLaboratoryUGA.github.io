      <section id="splash">
        <div class="ani-section">
          <img src="/images/logos/ssrl_logo_long.png" class="img-fluid" alt="Responsive image">
          <!-- <h1 class="center">UGA Small Satellite Research Laboratory</h1> -->
          <br><br>
          <div class="container">
            <div class="row">
              <div class="col-sm">
                <img src="/images/logos/unp_logo3.png" class="img-fluid" alt="Responsive image">
              </div>
              <div class="col-sm">
                <img src="/images/logos/nasa_logo.png" class="img-fluid" alt="Responsive image">
              </div>
              <div class="col-sm">
